{
  "name": "darkterminal/escpos-printer-server",
  "description": "The fck'in simple escpos printer server",
  "type": "library",
  "version": "1.0.6",
  "require": {
    "workerman/workerman": "^5.1",
    "mike42/escpos-php": "^4.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Darkterminal\\EscposPrinterServer\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "scripts": {
    "build": "sh ./build-phar.sh",
    "release:patch": "./release.sh patch",
    "release:minor": "./release.sh minor",
    "release:major": "./release.sh major"
  },
  "bin": "./eps",
  "authors": [
    {
      "name": "Imam Ali Mustofa",
      "email": "darkterminal@duck.com"
    }
  ],
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/darkterminal"
    },
    {
      "type": "saweria",
      "url": "https://saweria.co/darkterminal"
    }
  ],
  "require-dev": {
    "bamarni/composer-bin-plugin": "^1.8"
  },
  "config": {
    "allow-plugins": {
      "bamarni/composer-bin-plugin": true
    }
  }
}
